<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="600.0" prefWidth="600.0" stylesheets="@../../css/main.css" VBox.vgrow="ALWAYS" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.libanminds.dialogs_controllers.ReturnSaleItemsController">
   <content>
      <VBox spacing="8.0">
         <children>
            <HBox alignment="BASELINE_LEFT" spacing="8.0">
               <VBox.margin>
                  <Insets left="8.0" right="8.0" />
               </VBox.margin>
               <children>
                  <TableView fx:id="soldItemsTable" editable="true" prefHeight="400.0" prefWidth="289.0">
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
                  <TableView fx:id="returnedItemsTable" editable="true" layoutX="10.0" layoutY="11.0" prefHeight="400.0" prefWidth="289.0">
                     <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TableView>
               </children>
            </HBox>
            <HBox alignment="TOP_RIGHT" spacing="16.0">
               <VBox.margin>
                  <Insets bottom="8.0" left="8.0" right="8.0" />
               </VBox.margin>
               <children>
                  <Region disable="true" opacity="0.0" prefWidth="300.0" HBox.hgrow="ALWAYS" />
                  <VBox minWidth="120.0">
                     <children>
                        <Label text="Refund:">
                           <font>
                              <Font name="System Bold" size="14.0" />
                           </font>
                        </Label>
                     </children>
                  </VBox>
                  <VBox minWidth="80.0">
                     <children>
                        <TextField fx:id="refundAmount" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <HBox alignment="CENTER_RIGHT" spacing="12.0">
               <VBox.margin>
                  <Insets bottom="8.0" left="12.0" right="12.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </content>
</ScrollPane>
